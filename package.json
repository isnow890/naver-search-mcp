{
  "name": "@isnow890/naver-search-mcp",
  "version": "1.0.4",
  "description": "Naver Search MCP Server",
  "main": "dist/index.js",
  "module": "./src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    ".smithery"
  ],
  "scripts": {
    "prebuild": "npx shx rm -rf dist",
    "build": "tsc && npx shx cp -r data dist/",
    "build:smithery": "npx @smithery/cli@latest build src/index.ts -o .smithery/index.cjs",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/src/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "fastmcp": "^3.1.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "shx": "^0.3.4",
    "typescript": "^5.3.3",
    "sury": "^10.0.0",
    "@valibot/to-json-schema": "^1.3.0",
    "effect": "^3.17.0"
  },
  "author": "isnow890",
  "license": "MIT",
  "bin": {
    "naver-search-mcp": "./.smithery/index.cjs"
  },
  "type": "module"
}
